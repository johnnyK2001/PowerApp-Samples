function bpfPractise(executionContext) {
    var formContext = executionContext.getFormContext();
    var activeStageId = Xrm.Page.data.process.getActiveStageId().getId();
    var InstanceID = Xrm.Page.data.process.getInstanceId()

    var record = {};
    record.wa_name = formContext.getAttribute("wa_name").getValue();

    Xrm.WebApi.createRecord("wa_stage2", record).then(
        function success(result) {
            var newId = result.id;

            var record = {};
            record.traversedpath = activeStageId + ",fe82fab6-b8e8-40f3-a56e-a2f425a71766"; // Text
            record["activestageid@odata.bind"] = "/processstages(" + newId + ")"; // Lookup
            record["bpf_wa_stage2id@odata.bind"] = "/wa_stage2s(4b9f8237-677c-ee11-8179-002248595bba)"; // Lookup

            Xrm.WebApi.updateRecord("wa_bpf_chellenge", InstanceID, record).then(
                function success(result) {
                    var updatedId = result.id;

                    var pageInput = {
                        pageType: "entityrecord",
                        entityName: "wa_stage2",
                        entityId: newId
                    };

                    Xrm.Navigation.navigateTo(pageInput).then(
                        function success() {

                        },
                        function error() {
                            // Handle errors
                        }
                    );

                },
                function (error) {
                    console.log(error.message);
                }
            );

        },
        function (error) {
            console.log(error.message);
        }
    );


}